		"singleQuote": false,
		"trailingComma": "all",
		"useTabs": true,
		"tabWidth": 4
	},
	"dependencies": {
		"@atomico/run": "^0.12.0",
		"@atomico/tsconfig": "^1.1.2",
		"@atomico/vite": "^2.11.3",
		"@vitest/browser": "^0.34.5",
