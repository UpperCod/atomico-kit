{
	"name": "@atomico/use-resize-observer",
	"description": "Series of utilities in hooks format to extend the operation of Atomico",
	"version": "1.0.0",
	"type": "module",
	"author": {
		"name": "UpperCod",
		"email": "uppercod.git@gmail.com",
		"url": "https://github.com/uppercod"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/atomicojs/hooks.git"
	},
	"bugs": {
		"url": "https://github.com/atomicojs/hooks/issues"
	},
	"homepage": "https://atomico.gitbook.io/doc/atomico/atomico-hooks",
	"scripts": {
		"build:compile": "tsc",
		"build:exports": "exports \"dist/**/*\"",
		"test": "vitest run"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@atomico/use-current-value": "*",
		"@atomico/use-ref-values": "*"
	},
	"peerDependencies": {
		"atomico": "^1.75.1"
	},
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"default": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./use-resize-observer": {
			"default": "./dist/use-resize-observer.js",
			"types": "./dist/use-resize-observer.d.ts"
		}
	},
	"typesVersions": {
		"*": {
			"use-resize-observer": [
				"dist/use-resize-observer.d.ts"
			]
		}
	}
}
