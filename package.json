{
  "name": "@atomico/kit",
  "description": "a repo to start with atomico",
  "version": "0.5.0",
  "editable": "https://github.com/atomicojs/atomico-kit/tree/master",
  "scripts": {
    "start": "estack dev {src,template}/**/*.{html,md};src/*.yaml --js estack.js",
    "build": "estack dev {src,template}/**/*.{html,md} docs --js estack.js",
    "build:npm": "estack build src/{hooks,components}/*/*.{js,ts};!**/*.test.{js,ts} ./ --js estack.js",
    "test": "web-test-runner src/**/*.test.{tsx,js,ts} --node-resolve --puppeteer",
    "test:w": "web-test-runner src/**/*.test.{tsx,js,ts} --node-resolve --puppeteer --watch",
    "prepare": "npm run build:npm"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "atomico": "^1.2.1"
  },
  "peerDependencies": {
    "atomico": "^1.2.1"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.1.5",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@web/dev-server-esbuild": "^0.2.4",
    "@web/test-runner": "^0.7.27",
    "@web/test-runner-puppeteer": "^0.6.3",
    "estack": "^0.17.12"
  },
  "estack": {
    "js": {
      "plugins": {
        "@rollup/plugin-sucrase": {
          "jsxPragma": "h",
          "production": true,
          "exclude": [
            "node_modules/**"
          ],
          "transforms": [
            "jsx",
            "typescript"
          ]
        }
      }
    }
  },
  "exports": {
    "import": "./"
  }
}
