{
  "name": "@atomico/hooks",
  "description": "Series of utilities in hooks format to extend the operation of Atomico",
  "version": "3.50.0",
  "type": "module",
  "workspaces": [
    "src/*"
  ],
  "author": {
    "name": "UpperCod",
    "email": "uppercod.git@gmail.com",
    "url": "https://github.com/uppercod"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/atomicojs/hooks.git"
  },
  "bugs": {
    "url": "https://github.com/atomicojs/hooks/issues"
  },
  "homepage": "https://atomico.gitbook.io/doc/atomico/atomico-hooks",
  "scripts": {
    "compile": "tsc",
    "dev": "exports \"src/**/*.{ts,js}\" --watch",
    "exports": "exports \"dist/**/*\" --main index",
    "test": "wtr src/**/*.test.js --node-resolve",
    "prepublishOnly": "npm run test && npm run compile && npm run exports"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "atomico": "^1.68.0"
  },
  "devDependencies": {
    "@atomico/exports": "^1.12.0",
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@types/chai": "^4.2.21",
    "@web/test-runner": "^0.13.16",
    "@web/test-runner-commands": "^0.5.12",
    "atomico": "^1.68.0",
    "typescript": "^4.7.2"
  }
}
